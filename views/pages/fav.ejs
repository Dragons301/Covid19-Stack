<!DOCTYPE html>
<html lang="en">

<head>
    <%- include ('../layout/head')  %>
</head>

<body>
    <%- include ('../layout/header') %>
    <main>
        <div id="div-news">

            <% result.forEach (obj => { %>
            <section id="news-section">
                <a id="news-title" href=" <%= obj.url %> ">
                    <h1 id="news-title"> <%= obj.title %> </h1>
                </a>

                <img id="news-img" src=" <%= obj.image %> " alt="">
                <p id="news-p"> <%= obj.description %> </p>
                <button class="delet-button">
                    <i class="fa fa-trash"></i> delet </button>

            </section>

            <% }); %>


        </div>



    </main>
    <%- include ('../layout/footer') %>
</body>

</html>